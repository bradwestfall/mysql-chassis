{
  "name": "mysql-chassis",
  "version": "4.1.0",
  "description": "Wrapper code for node-mysql",
  "main": "dist/index.js",
  "repository": {
    "url": "https://github.com/bradwestfall/node-mysql-chassis.git"
  },
  "jsnext:main": "dist/mysql-chassis.es2015.js",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --out-dir dist",
    "example:insert": "babel-node ./example/insert.js",
    "example:select": "babel-node ./example/select.js",
    "test": "mocha --compilers js:babel-register"
  },
  "keywords": [
    "mysql"
  ],
  "author": "Brad Westfall",
  "maintainers": [
    "Brad Westfall <brad@azpixels.com>",
    "Kevin Lanni <therealklanni@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "mysql": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.18.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "mocha": "^3.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "standard": "^8.3.0"
  }
